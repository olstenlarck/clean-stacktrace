{
  "name": "clean-stacktrace",
  "version": "1.0.0",
  "description": "Clean up error stack traces - just a fork of [clean-stack][]. Working on node.js v0.10 and above.",
  "repository": "tunnckoCore/clean-stacktrace",
  "author": "Sindre Sorhus <sindresorhus@gmail.com> (sindresorhus.com)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100 --branches 100 --statements 100 --functions 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify"
  },
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.14",
    "mukla": "0.4.7",
    "nyc": "^8.3.2",
    "pre-commit": "^1.1.3",
    "standard": "^8.5.0",
    "standard-version": "^3.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "clean",
    "stack",
    "trace",
    "traces",
    "error",
    "err"
  ],
  "verb": {
    "run": true,
    "toc": true,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "clean-stack",
        "stack-utils",
        "stacktrace-metadata",
        "error-base",
        "error-format"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
